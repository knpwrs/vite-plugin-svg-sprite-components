{
  "name": "vite-plugin-svg-sprite-components-preact",
  "type": "module",
  "version": "0.0.0",
  "description": "Create svg sprite sheets from imported svg files and get usable Preact components.",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "check": "run-s check:*",
    "check:eslint": "eslint src",
    "check:prettier": "prettier --check .",
    "check:ts": "tsc --noEmit",
    "prepack": "run-s clean build",
    "dev": "vite ./test",
    "start": "vite build ./test && vite preview ./test"
  },
  "keywords": [
    "vite-plugin",
    "svg",
    "sprites"
  ],
  "author": "Ken Powers <ken@kenpowers.net> (https://knpw.rs)",
  "license": "UNLICENSE",
  "dependencies": {
    "preact": "^10.19.2",
    "proper-tags": "^2.0.2",
    "vite-plugin-svg-sprite-components-core": "workspace:*"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@tsconfig/strictest": "^2.0.2",
    "@types/node": "^20.10.3",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "tiny-invariant": "^1.3.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.5"
  }
}
