{
  "name": "vite-plugin-svg-sprite-components-react",
  "type": "module",
  "version": "0.0.0",
  "description": "Create svg sprite sheets from imported svg files and get usable React components.",
  "exports": {
    ".": "./dist/index.js",
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "check": "run-s check:*",
    "check:eslint": "eslint src",
    "check:prettier": "prettier --check .",
    "check:ts": "tsc --noEmit",
    "prepack": "run-s clean build"
  },
  "keywords": [
    "vite-plugin",
    "svg",
    "sprites"
  ],
  "author": "Ken Powers <ken@kenpowers.net> (https://knpw.rs)",
  "license": "UNLICENSE",
  "homepage": "https://github.com/knpwrs/vite-plugin-svg-sprite-components/",
  "repository": {
    "type": "git",
    "url": "https://github.com/knpwrs/vite-plugin-svg-sprite-components.git"
  },
  "dependencies": {
    "@types/react": "^18.2.42",
    "proper-tags": "^2.0.2"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@tsconfig/strictest": "^2.0.2",
    "@types/node": "^20.10.3",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2",
    "vite-plugin-svg-sprite-components-core": "workspace:^"
  }
}
